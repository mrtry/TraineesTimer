{
  "compilerOptions": {
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "alwaysStrict": true,
    "baseUrl": "src",
    "jsx": "react-native",
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "preserveConstEnums": true,
    "pretty": true,
    "rootDir": "src",
    "sourceMap": true,
    "strictNullChecks": true,
    "target": "es2017"
  }
}